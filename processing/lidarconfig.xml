<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<process-config name="Raw LiDAR data filter and processing">
    <input name="mainIn">
        <archiveOnly>true</archiveOnly>
        <filePattern>
            <directory>sharedstorage</directory>
            <midfix/>
            <format>pcd</format>
        </filePattern>
    </input>
    <input name="calibrationIn">
        <archiveOnly>true</archiveOnly>
        <filePattern>
            <directory>sharedstorage</directory>
            <midfix>.calibration</midfix>
            <format>pcd</format>
        </filePattern>
    </input>
    <initialCameraParams>
        <pos_x>-5</pos_x>
        <pos_y>4.5</pos_y>
        <pos_z>0</pos_z>
        <view_x>30</view_x>
        <view_y>-30</view_y>
        <view_z>0</view_z>
        <up_x>-10</up_x>
        <up_y>10.5</up_y>
        <up_z>10</up_z>
    </initialCameraParams>
    <drawLines>
        <line>
            <name>Median Divider Ground</name>
            <x1>10.75</x1>
            <y1>-3.75</y1>
            <z1>-3.5</z1>
            <x2>17</x2>
            <y2>-15.5</y2>    
            <z2>6</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Roadside Divider Ground</name>
            <x1>3</x1>
            <y1>-10</y1>
            <z1>-4.75</z1>
            <x2>8.7</x2>
            <y2>-23</y2>
            <z2>5</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Close Ground Connector</name>
            <x1>3</x1>
            <y1>-10</y1>
            <z1>-4.75</z1>
            <x2>10.75</x2>
            <y2>-3.75</y2>
            <z2>-3.5</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Far Ground Connector</name>
            <x1>17</x1>
            <y1>-15.5</y1>    
            <z1>6</z1>
            <x2>8.7</x2>
            <y2>-23</y2>
            <z2>5</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
    </drawLines>
    <passThroughFilter>
        <enabled>false</enabled>
        <filter_x_min>0.0</filter_x_min>
        <filter_x_max>15</filter_x_max>
        <filter_z_min>-1</filter_z_min>
        <filter_z_max>3</filter_z_max>
        <filter_y_min>-12.5</filter_y_min>
        <filter_y_max>12.5</filter_y_max>
    </passThroughFilter>
    <boxFilter>
        <enabled>false</enabled>
        <minPoint>
            <x>0.0</x>
            <y>0.0</y>
            <z>-20.0</z>
        </minPoint>
        <maxPoint>
            <x>7.7</x>
            <y>21.34</y>
            <z>20.0</z>
        </maxPoint>
        <transformation>
            <x>-5.0</x>
            <y>8.4</y>
            <z>0.0</z>
            <roll>0.0</roll>
            <pitch>0.0</pitch>
            <yaw>-0.7328</yaw>
        </transformation>
    </boxFilter>
    <backgroundFilter>
        <enabled>false</enabled>
        <threshold>0.5</threshold>
    </backgroundFilter>
</process-config>
