<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<process-config name="Raw LiDAR data filter and processing">
    <input name="mainIn">
        <archiveOnly>true</archiveOnly>
        <filePattern>
            <directory>sharedstorage</directory>
            <midfix/>
            <format>pcd</format>
        </filePattern>
    </input>
    <input name="calibrationIn">
        <archiveOnly>true</archiveOnly>
        <filePattern>
            <directory>sharedstorage</directory>
            <midfix>.calibration</midfix>
            <format>pcd</format>
        </filePattern>
    </input>
    <initialCameraParams>
        <pos_x>-10</pos_x>
        <pos_y>40</pos_y>
        <pos_z>5</pos_z>
        <view_x>30</view_x>
        <view_y>-5</view_y>
        <view_z>-2</view_z>
        <up_x>-10</up_x>
        <up_y>40</up_y>
        <up_z>100</up_z>
    </initialCameraParams>
    <passThroughFilter>
        <enabled>true</enabled>
        <filter_x_min>0.0</filter_x_min>
        <filter_x_max>15</filter_x_max>
        <filter_z_min>-1</filter_z_min>
        <filter_z_max>3</filter_z_max>
        <filter_y_min>-12.5</filter_y_min>
        <filter_y_max>12.5</filter_y_max>
    </passThroughFilter>
    <boxFilter>
        <enabled>true</enabled>
        <minPoint>
            <x>0.0</x>
            <y>0.0</y>
            <z>-20.0</z>
        </minPoint>
        <maxPoint>
            <x>7.7</x>
            <y>21.34</y>
            <z>20.0</z>
        </maxPoint>
        <transformation>
            <x>-5.0</x>
            <y>8.4</y>
            <z>0.0</z>
            <roll>0.0</roll>
            <pitch>0.0</pitch>
            <yaw>-0.7328</yaw>
        </transformation>
    </boxFilter>
    <backgroundFilter>
        <enabled>true</enabled>
        <targetDistanceThreshold>0.5</targetDistanceThreshold>
    </backgroundFilter>
</process-config>
