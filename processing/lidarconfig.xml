<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<process-config name="Raw LiDAR data filter and processing">
    <input name="mainIn">
        <archiveOnly>true</archiveOnly>
        <filePattern>
            <directory>sharedstorage</directory>
            <midfix/>
            <format>pcd</format>
        </filePattern>
    </input>
    <input name="calibrationIn">
        <archiveOnly>true</archiveOnly>
        <filePattern>
            <directory>sharedstorage</directory>
            <midfix>.calibration</midfix>
            <format>pcd</format>
        </filePattern>
    </input>
    <initialCameraParams>
        <pos_x>-5</pos_x>
        <pos_y>4.5</pos_y>
        <pos_z>0</pos_z>
        <view_x>30</view_x>
        <view_y>-30</view_y>
        <view_z>0</view_z>
        <up_x>-10</up_x>
        <up_y>10.5</up_y>
        <up_z>10</up_z>
    </initialCameraParams>
    <drawLines>
        <line>
            <name>Median Divider Ground</name>
            <!-- Point 0 (Close Left Ground)-->
            <x1>10.75</x1>
            <y1>-3.75</y1>
            <z1>-3.5</z1>
            <!-- Point 1 (Far Left Ground)-->
            <x2>17</x2>
            <y2>-15.5</y2>    
            <z2>6</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Median Divider Overhead</name>
            <!-- Point 4 (Close Left Overhead)-->
            <x1>7.55</x1>
            <y1>-1.25</y1>
            <z1>3.5</z1>
            <!-- Point 5 (Far Left Overhead)-->
            <x2>13.75</x2>
            <y2>-12.25</y2>    
            <z2>12</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Far Left Vertical Connector</name>
            <!-- Point 1 (Far Left Ground)-->
            <x1>17</x1>
            <y1>-15.5</y1>    
            <z1>6</z1>
            <!-- Point 5 (Far Left Overhead)-->
            <x2>13.75</x2>
            <y2>-12.25</y2>    
            <z2>12</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Roadside Divider Ground</name>
            <!-- Point 2 (Close Right Ground)-->
            <x1>3</x1>
            <y1>-10</y1>
            <z1>-4.75</z1>
            <!-- Point 3 (Far Right Ground)-->
            <x2>8.7</x2>
            <y2>-23</y2>
            <z2>5</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Roadside Divider Overhead</name>
            <!-- Point 6 (Close Right Overhead)-->
            <x1>0.75</x1>
            <y1>-7</y1>
            <z1>2.75</z1>
            <!-- Point 7 (Far Right Overhead)-->
            <x2>6</x2>
            <y2>-18.75</y2>
            <z2>11</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Close Right Vertical Connector</name>
            <!-- Point 2 (Close Right Ground)-->
            <x1>3</x1>
            <y1>-10</y1>
            <z1>-4.75</z1>
            <!-- Point 6 (Close Right Overhead)-->
            <x2>0.75</x2>
            <y2>-7</y2>
            <z2>2.75</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Far Right Vertical Connector</name>
            <!-- Point 3 (Far Right Ground)-->
            <x1>8.7</x1>
            <y1>-23</y1>
            <z1>5</z1>
            <!-- Point 7 (Far Right Overhead)-->
            <x2>6</x2>
            <y2>-18.75</y2>
            <z2>11</z2>
            <r>255</r>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Close Vertical Connector</name>
            <!-- Point 0 (Close Left Ground)-->
            <x1>10.75</x1>
            <y1>-3.75</y1>
            <z1>-3.5</z1>
            <!-- Point 4 (Close Left Overhead)-->
            <x2>7.55</x2>
            <y2>-1.25</y2>
            <z2>3.5</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Close Ground Connector</name>
            <!-- Point 2 (Close Right Ground)-->
            <x1>3</x1>
            <y1>-10</y1>
            <z1>-4.75</z1>
            <!-- Point 0 (Close Left Ground)-->
            <x2>10.75</x2>
            <y2>-3.75</y2>
            <z2>-3.5</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Far Ground Connector</name>
            <!-- Point 1 (Far Left Ground)-->
            <x1>17</x1>
            <y1>-15.5</y1>    
            <z1>6</z1>
            <!-- Point 3 (Far Right Ground)-->
            <x2>8.7</x2>
            <y2>-23</y2>
            <z2>5</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Close Overhead Connector</name>
            <!-- Point 4 (Close Left Overhead)-->
            <x1>7.55</x1>
            <y1>-1.25</y1>
            <z1>3.5</z1>
            <!-- Point 6 (Close Right Overhead)-->
            <x2>0.75</x2>
            <y2>-7</y2>
            <z2>2.75</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Far Overhead Connector</name>
            <!-- Point 5 (Far Left Overhead)-->
            <x1>13.75</x1>
            <y1>-12.25</y1>    
            <z1>12</z1>
            <!-- Point 7 (Far Right Overhead)-->
            <x2>6</x2>
            <y2>-18.75</y2>
            <z2>11</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Threshold Line Ground</name>
            <!-- Internal division 3:7 on median divider (70% threshold)-->
            <x1>12.625</x1>
            <y1>-7.275</y1>
            <z1>-0.65</z1>
            <!-- Internal division 3:7 on roadside divider (70% threshold)-->
            <x2>4.71</x2>
            <y2>-13.9</y2>
            <z2>-1.825</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Threshold Line Overhead</name>
            <!-- Internal division 3:7 on median divider overhead (70% threshold)-->
            <x1>9.41</x1>
            <y1>-4.55</y1>
            <z1>6.05</z1>
            <!-- Internal division 3:7 on roadside divider overhead (70% threshold)-->
            <x2>2.325</x2>
            <y2>-10.525</y2>
            <z2>5.225</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Threshold Connector Left</name>
            <x1>12.625</x1>
            <y1>-7.275</y1>
            <z1>-0.65</z1>
            <x2>9.41</x2>
            <y2>-4.55</y2>
            <z2>6.05</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
        <line>
            <name>Threshold Connector Right</name>
            <x1>4.71</x1>
            <y1>-13.9</y1>
            <z1>-1.825</z1>
            <x2>2.325</x2>
            <y2>-10.525</y2>
            <z2>5.225</z2>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </line>
    </drawLines>
    <passThroughFilter>
        <enabled>false</enabled>
        <filter_x_min>0.0</filter_x_min>
        <filter_x_max>15</filter_x_max>
        <filter_z_min>-1</filter_z_min>
        <filter_z_max>3</filter_z_max>
        <filter_y_min>-12.5</filter_y_min>
        <filter_y_max>12.5</filter_y_max>
    </passThroughFilter>
    <boxFilter>
        <enabled>true</enabled>
        <minPoint>
            <x>0</x>
            <y>0</y>
            <z>0</z>
        </minPoint>
        <maxPoint>
            <x>10</x>
            <y>16.4</y>
            <z>8.09</z>
        </maxPoint>
        <transformation>
            <x>4</x>
            <y>22</y>
            <z>9</z>
            <roll>0.45</roll>
            <pitch>0.475</pitch>
            <yaw>-0.45</yaw>
        </transformation>
    </boxFilter>
    <backgroundFilter>
        <enabled>true</enabled>
        <threshold>0.5</threshold>
    </backgroundFilter>
    <!-- Set to true for debugging -->
    <exitAfterLastFrame>true</exitAfterLastFrame>
</process-config>
